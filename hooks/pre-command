CONDITIONAL_PIPELINE_SLUG="$BUILDKITE_PLUGIN_CONDITIONAL_PIPELINE_BUILDER_CONDITIONAL_PIPELINE_SLUG"
echo "CONDITIONAL_PIPELINE_SLUG is set to ${CONDITIONAL_PIPELINE_SLUG}"

buildkite-agent meta-data set "RUN_LINT" "true"

changed_files=$(git diff --name-only origin/main...HEAD)

echo $changed_files
